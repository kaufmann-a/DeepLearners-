{
    "environment": {
        "name" : "Old model can be removed when new is running",
        "output_path": "getenv('OUTPUT_PATH')", //Creates a folder with name output_path in this folder traininglogs etc. will be saved
        "log_file" : "logs.txt" //Name of the log file
    },
    "data_collection" : {
        "folder" : "../data/",
        "dataset" : ["h36m", "mpii"],
        "augmentations" : {
            "scale_factor" : 0.25,
            "rotation_factor" : 30,
            "color_factor": 0.2,
            "random_flip" : true,
            "voc_occluder_datapath" : "getenv('VOC_DATASET')",
            "voc_occluder" : false,
            "voc_occluder_p" : 0.5,
        },
        "h36m_params" : {
            "train_set": "train",
            "val_set": "val",
            "num_joints" : 17,
            "num_cams": 4,
        },
         "mpii_params" : {
            "train_set": "train",
            "val_set": "valid",
            "num_joints" : 16,
            "num_cams": 1,
        },
        "image_size": [256, 256],
        "z_weight" : 1.0,
    },
    "training": {
        "general" : {
            "batch_size": 32,
            "num_epochs" : 140,
            "checkpoint_save_interval" : 20,
            "num_workers" : 10,
            "shuffle_data" : true,
            "log_to_comet" : true,
        },
        "model": {
            "name": "ResPoseNet_DeconvHead",  //  IntegralPoseRegressionModel, ResPoseNet_Regression, PoseAlexNetReg
            "num_joints" : 17,
            "resnet_model" : "resnet50", //"resnet18", "resnet34", "resnet101", "resnet152"
            "num_deconv_layers" : 3,
            "num_deconv_filters" : 256,
            "kernel_size" : 4,
            "depth_dim" : 64
        },
        "optimizer": {
            "name": "adam",
            "lr": 0.0001,
            "beta1": 0.9,
            "beta2": 0.999
        },
        "loss": {
            "loss_function": "IntegralJointLocationLoss", // L1JointRegressionLoss
            "norm" : false,
            "loss_type": "L1",
            "output_3d": true,
        },
        "lr_scheduler": {
            "name": "multiStepLR",
            "stepLR": {
                "step_size" : 10,
                "gamma" : 0.1,
            },
            "multiStepLR": {
                "milestones" : [90,120],
                "gamma" : 0.1,
            }
        },
    },
    "inference" : {
        "general" : {
            "foreground_threshold" : 0.25,
            "use_original_image_size" : false,
        }
    }
}
